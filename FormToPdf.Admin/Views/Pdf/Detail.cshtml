@using Microsoft.AspNetCore.Identity
@inject SignInManager<IdentityUser> SignInManager
@inject UserManager<IdentityUser> UserManager
@model PdfDetailViewModel

@{
    ViewData["Title"] = "Pdf Detail";
}

<section id="pdf-detail" class="container mx-auto position-relative">
    <div class="pdf-sidebar">

    </div>
    <div class="row mt-5">
        <div class="col-md-8">
            <img class="img-fluid" src="data:image/jpg;base64, @Model.Base64Image" alt="@Model.Pdf.Name" />
        </div>
    </div>
</section>
